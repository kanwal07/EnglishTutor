<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tense">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.StartUP.StartUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TenseActivity" />
        <activity android:name=".activity.ShowExample" />
        <activity android:name=".activity.ChooseTense" />
        <activity android:name=".activity.TutorialActivity" />
        <activity
            android:name=".activity.ShowQuestion.ShowQuestionViewPager"
            android:label="@string/title_activity_show_question_view_pager"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.Login.LoginActivity">

        </activity>
        <activity android:name=".activity.MainActivity"></activity>
    </application>

</manifest>